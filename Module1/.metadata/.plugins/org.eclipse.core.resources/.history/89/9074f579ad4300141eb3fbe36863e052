#ifndef PADDLE_H_
#define PADDLE_H_

const int leftBound = 0;
const int rightBound = 320;
const int moveScale = 1;
const int paddleHeight=10;

typedef struct Paddle
{
	float x;
	float y=paddleHeight;

} Paddle;

extern void moveHorizontal(Paddle *paddle, int x);


#endif /* PADDLE_H_ */
