#include "../Headers/Ball.h"

*Ball startBall(float x, float y, float xVelo, float yVelo)
{
	Ball ball;
	ball.x = x;
	ball.y = y;
	ball.xVelo = xVelo;
	ball.yVelo = yVelo;
	return ball;
}

void updatePosition(Ball *ball)
{
	ball->x += ball->xVelo;
	ball->y += ball->yVelo;
}
